<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="it.alessios.jtaskslogger.controller.NewTaskDialogController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="82.0" text="Task Name: " AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0" />
      <TextField fx:id="taskName" layoutX="97.0" layoutY="10.0" AnchorPane.leftAnchor="97.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0" />
      <ButtonBar layoutX="45.0" layoutY="41.0" prefHeight="40.0" prefWidth="261.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="41.0">
        <buttons>
          <Button fx:id="btnOk" defaultButton="true" mnemonicParsing="false" onAction="#handleOk" text="OK" />
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
